        "# Step 2: Data Cleaning\n",
        "# Drop missing values\n",
        "data = data.dropna()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 1000
        },
        "id": "EeEaNoVy6Jkv",
        "outputId": "535ae590-8e2f-4b9d-92fd-6f796888324c"
